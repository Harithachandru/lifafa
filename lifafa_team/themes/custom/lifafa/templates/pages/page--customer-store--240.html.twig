                    <div  class="user-in user-in-black">
                        <ul>                            
                            {% if logged_in %}
                                <li><a href="{{store_base_url}}">My account</a></li>
                                <li><a href="{{store_home_url}}">My store</a></li>

                                <li><a href="{{logoutUrl}}" title="Logout">Logout</a></li>
                                {% else %}
                                <li><a href="{{base_url}}/user/login?store_id={{field_store_id}}" title="Sign in">Sign in</a></li>
                                    {#{% if external_url %}
                                    <li><a href="{{base_url}}{{external_url}}" title="Sign up">Sign up</a></li>
                                    {% else %}
                                    <li><a href="{{getStoreBaseUrl}}myaccount/{{field_store_id}}/self/register" title="Sign up">Sign up</a></li>
                                    {% endif %}#}
                                {% endif %}                            
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </header>
    
</section>

