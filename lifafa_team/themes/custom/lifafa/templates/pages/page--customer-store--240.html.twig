            <div class="top-nav ">
                <div class="link-hone">
                    
                    {% if logged_in %}
                        <a href="{{store_home_url}}">
                            <img src="{{ base_path ~ directory }}/images/house-black.png" alt="logo">
                        </a>
                    {% else %}
                        <a href="{{base_url}}/user/login?store_id={{field_store_id}}">
