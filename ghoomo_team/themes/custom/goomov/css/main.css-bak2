.list:before {
    content: "";
    width: 10px;
    height: 10px;
    background: #fff;
    display: inline-block;
    vertical-align: middle;
    border-radius: 10px;
    margin: -4.5px 10px 0 0;
}
.banner.banner-slider { height:auto;}
.banner.banner-slider .banner-img { position: relative;}
.banner.banner-slider .item {position: relative;}
.banner-slider .slick-dots {
    margin: 0;
    position: absolute;
    bottom: 20px;
    left: 50%;
    transform: translateX(-50%);
    padding: 0;
    list-style: none;
}
.banner-slider .slick-dots li {
    padding: 0 10px;
    float: left;
}
.banner-slider .slick-dots button {
    border: none;
    background: #fff;
    width: 20px;
    height: 20px;
    border-radius: 10px;
    font-size: 0px;
    float: left;
}
.banner-slider .slick-dots li.slick-active button {
    background: #01556F;
}
