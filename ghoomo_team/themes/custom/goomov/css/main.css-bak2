body{margin:0; color:#000; font-family: 'Verdana'; font-size:15px;outline:none;line-height:1.4;}
ul{margin:0 0 30px 0;text-decoration:none;}
ol{margin:0 0 30px 0}
p{margin:0 0 30px 0;color:#000;}
h6, .h6, h5, .h5, h4, .h4, h3, .h3, h2, .h2, h1, .h1 {font-family: 'Verdana-Bold';}
h1{line-height:1;color:#fff;margin:0 0 25px 0;text-transform:uppercase;}
h2{font-size:40px;color:#01556F;margin:0 0 20px 0;line-height:1.5;}
h2 span {font-size:36px; }
h2 a {color:#01556F;}
h3{font-size:36px;color:#fff;margin:0 0 20px 0;line-height:1.1;}
h4{font-size:20px;color:#000;margin:0 0 20px 0;line-height:1.1;}
h5{font-size:18px;color:#000;margin:0 0 5px 0;line-height:1;line-height:1.2;}
h6{font-size:15px;color:#000;margin:0 0 10px 0;line-height:1.1;}
button{cursor:pointer;}
form {    font-family: 'Verdana';}
select {    font-family: 'Verdana';}
input {    font-family: 'Verdana';}
textarea {    font-family: 'Verdana';}
a{color:#05aff2;cursor:pointer;color:#54bcfc;}
a, a:hover, a:focus{text-decoration:none;}
img{max-width:100%;vertical-align:middle;border:0;}
.clear{clear:both}
.btn{color:#ffffff;border-radius:0px;padding:10px 15px;display:inline-block;margin-bottom:0;text-align:center;vertical-align:middle;-ms-touch-action:manipulation;touch-action:manipulation;cursor:pointer;white-space:nowrap;-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none;user-select:none;line-height:20px;font-size:16px;border:none;min-width:110px;box-shadow:0 3px 6px rgba(0,0,0,0.2);}
.btn-primary{background:#FFA300;border-color:#FFA300;color:#fff !important;}
.btn-primary:hover{border-color:#fcbb6c;background-color:#fcbb6c;color:#000000 !important;}
.btn-primary:visited:hover{border-color:#fcbb6c;background-color:#fcbb6c;color:#000000 !important;}
.btn-primary:active:hover, .btn-primary.active:hover, .open>.dropdown-toggle.btn-primary:hover, .btn-primary:active:focus, .btn-primary.active:focus, .open>.dropdown-toggle.btn-primary:focus, .btn-primary:active.focus, .btn-primary.active.focus, .open>.dropdown-toggle.btn-primary.focus{border-color:#fcbb6c;background-color:#fcbb6c;color:#000000 !important;}
.container{width:inherit;max-width:1240px; padding:0 20px; margin:0 auto;position:relative;}
.text-center { text-align: center;}
.pb-10{padding-bottom:10px !important;;}
.pb-15{padding-bottom:15px !important;}
.pb-20{padding-bottom:20px !important;}
.pb-25{padding-bottom:25px !important;}
.pb-30{padding-bottom:30px !important;}
.pt-10{padding-top:10px !important;}
.pt-15{padding-top:15px !important;}
.pt-20{padding-top:20px !important;}
.pt-25{padding-top:25px !important;}
.pt-30{padding-top:30px !important;}
.flex-wrap {  -webkit-flex-wrap: wrap;  -ms-flex-wrap: wrap;  flex-wrap: wrap;}
.row{ width: 100%;  display: -webkit-box;  display: -webkit-flex; display: -ms-flexbox; display: flex; }
.column { position: relative;  min-height: 1px; display: -webkit-box; display: -webkit-flex; display: -ms-flexbox; display: flex; }
.column-middle{ align-content: center;   align-items: center;}
.text-1 { font-size: 24px;}

.top-border { border-top: 2px solid #707070; max-width: 250px;  margin: 0 auto 35px auto;}
.column-25 { width: 25%;}
.column-50 { width: 50%;}
.row-20 { margin: 0 -20px;     width: auto;}
.column-p20 { padding: 0 20px;}

/* Header CSS Start */

.top-nav{padding:40px 0 20px 0; border-bottom:2px solid #01556F; position:relative;}
header.header-top-nav{position:relative;z-index:9;}
.logo img{max-height:60px;}
.menu-wrap{ padding-left: 40px;}
.header-column-left { width: 80%}
.header-column-right {width: 20%;  justify-content: flex-end;}
.menu ul{list-style:none;padding:0;margin:0; }
.menu li{float:left;padding:0 20px;position:relative;border-right:1px solid #000000; line-height:1.2;text-transform:uppercase; font-family: 'Verdana-Bold';}
.menu li:last-child{border:none;}
.menu li:hover { cursor: pointer;}
.menu li a{color:#000000; display:block;}
.menu li a:hover{color:#01556F;}
.menu li.menu-item--active-trail a{color:#01556F;}

.menu li.has-sub ul{position:absolute;left:0;top:100%;background:#FFFFFF;border:1px solid #eaeaea;box-shadow:0 3px 2px rgba(0,0,0, 0.75);display:none !important;z-index:99;}
.menu li.has-sub:hover>ul{display:block !important;}
.menu li.has-sub ul li{width:100%;border:none;position:relative;font-size:16px;padding:10px 15px;text-transform:none;border-bottom:2px solid #b7b7b7; font-family: 'Verdana'; color:#000;}
.menu li.has-sub ul li:last-child { border: none;}
.menu li.has-sub ul li:hover {background:#01556F; color:#fff;}
.menu li.has-sub ul li a{font-size:16px;white-space:pre;width:100%;text-transform:none; font-family: 'Verdana'; color:#000;}
.menu li.has-sub ul li:hover a{background:#01556F;color:#fff;}
.menu li.has-sub ul li:hover>a{background:#01556F;color:#fff;}
.menu li.has-sub ul li ul{left:100%;top:0;}
.search-row{padding:20px 0 0 0;display:none;}
.search{max-width:860px;border:1px solid #01556F;border-radius:5px;height:40px;overflow:hidden;margin:0 auto;}
.se-country { margin-right:20px;}
.se-country select { border: 0px; text-transform: uppercase; }
.s-btn{float:left;cursor:pointer; margin-left:20px;}
.s-btn img{width:30px;height:30px;}
.search .gsearch{padding:5px 20px;outline:none;height:38px;color:#01556F; font-size:21px;line-height:30px;float:left;border:none;border-radius:5px;width:calc(100% - 45px);}
.top-searchbox{}
.search .submit-btn{background:url(../images/search-icon-m.png) no-repeat left center;border:none;height:40px;float:left;width:44px;font-size:0px; }
.user-in{background:url(../images/user.png) no-repeat center bottom;border:1px solid #FAA21B;outline:none;height:30px;width:30px;border-radius:20px;float:left;position:relative;cursor:pointer;}
.user-in ul{position:absolute;right:0;top:100%;background:#FFFFFF;border:1px solid #eaeaea;box-shadow:0 3px 2px rgba(0,0,0, 0.2);padding:0;margin:0;list-style:none;z-index:99;min-width:170px;display:none;}
.user-in:hover ul{display:block;}
.user-in ul li{}
.user-in ul li a{padding:10px 16px;font-family:'Verdana';font-size:16px;white-space:pre;border-bottom:2px solid #D4D4D4;display:block;color:#000 }
.user-in ul li:last-child a{border-bottom:2px solid #fff;}
.user-in ul li a:hover{border-bottom:2px solid #01556F;background:#01556F;color:#fff;}

/* Header CSS  END*/

/* Footer CSS Start */

.footer { background: #01556F;padding: 45px 0;}
.footer-menu {padding: 20px 0 0 0;}
.footer-menu ul { list-style: none;padding: 0;margin: 0;}
.footer-menu li {  border-right: 1px solid #fff;  display: inline-block; vertical-align: middle; padding: 0 20px; }
.footer-menu li:last-child { border: none;}
.footer-menu li a {display: block;color: #fff; line-height: 1.2;}
.copyright {color: #fff;padding: 25px 0 0 0;}

/* Footer CSS  END*/

/* Banner CSS Start */

.banner { position:relative; height: 600px; margin-top: 20px;}
.banner .banner-img:before { content:""; position:absolute; left: 0; top:0; background: rgba(0,0,0,0.6); width: 100%; height: 100%; }
.overlay:before {
    content: "";
    position: absolute;
    background: rgba(0,0,0,0.25);
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 10;
}
.banner-img { position:absolute;  left: 0; width: 100%; top:0; height: 100%;}
.banner-img img { height: 100%; width: 100%; object-fit: cover;}
.banner-text { position:absolute;  left: 0; width: 100%; top:50%; transform: translateY(-50%); color:#fff;}
.banner-text h2 { color: #fff;}
/* Banner CSS  END*/

/* Section 1 CSS Start */
.sec1 { padding: 30px 0; }
