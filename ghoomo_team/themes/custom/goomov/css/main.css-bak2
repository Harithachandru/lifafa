/* sign in CSS Start */
.sign-in { padding: 30px 0; }
.tablinks { border: 0px; background:none; font-size: 18px;  text-transform: uppercase; margin: 0 10px; padding:0 0 5px 0; border-bottom: 2px solid transparent}
.tabcontent {  display: none;}
.tab button:hover { color: #01556F; border-bottom: 2px solid #01556F}
.tab button.active {color: #01556F;border-bottom: 2px solid #01556F}
.form-row { padding: 0 0 20px 0;}
input.fm-in{border:1px solid #707070;border-radius:3px;height:40px;width:100%;color:#707070;padding:10px;font-size:16px;outline:none;font-family:Arial, Helvetica, sans-serif; background:#fff;}
textarea.fm-in{border:1px solid #707070;border-radius:3px;height:85px;width:100%;color:#707070;padding:10px;font-size:16px;outline:none;font-family:Arial, Helvetica, sans-serif; background:#fff;}
select.fm-in{border:1px solid #707070;border-radius:3px;height:38px;width:100%;color:#707070;padding:10px;font-size:12px;outline:none;font-family:Arial, Helvetica, sans-serif; background:#fff;}
/*.sign-in-form { padding: 20px 0}*/
.otp-time { font-size: 16px;  text-align: right; padding: 0 0 20px 0;}
/* sign in CSS  END*/

.banner-col-50 { max-width: 50%;}

.custom-system-message{text-align:center;margin-top:10px;}
